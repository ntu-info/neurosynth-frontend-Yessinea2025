          const text =3D await resp.text();
          out.textContent =3D info + '\n' + text;
          return;
        }

        if (ct.includes('application/json')) {
          const data =3D await resp.json();
          out.textContent =3D info + '\n' + JSON.stringify(data, null, 2);
        } else {
          const text =3D await resp.text();
          out.textContent =3D info + '\n' + text;
        }
